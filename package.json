    "connect-history-api-fallback": "^1.6.0",
    "date-fns": "^2.14.0",
    "dotenv-flow": "^3.1.0",
    "express": "^4.16.4",
    "mongodb": "^3.5.9",
    "setlistfm-js": "^1.2.0",
    "spotify-web-api-node": "^4.0.0",
    "underscore": "^1.10.2",
    "vue": "^2.6.11",
    "vue-js-toggle-button": "^1.3.3",
    "vue-object-merge": "^0.1.8",
    "vue-router": "^3.0.1",
    "vue-typeahead-bootstrap": "^2.6.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^3.0.1",
