    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "popper.js": "^1.14.6",
    "setlistfm-js": "^1.2.0",
    "spotify-web-api-node": "^4.0.0",
    "underscore": "^1.10.2",
    "vue": "^2.5.17",
    "vue-axios": "^2.1.4",
    "vue-bootstrap-typeahead": "^0.2.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
